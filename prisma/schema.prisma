
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:rootpassword@db:3306/mydatabase"
}

model Currency {
  id         Int      @id @default(autoincrement())
  code       String   @unique 
  name       String   
  symbol     String   
  rate       Decimal  
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
