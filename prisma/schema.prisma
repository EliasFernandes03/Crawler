
generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Currency {
  id         Int      @id @default(autoincrement())
  code       String   @unique 
  name       String   
  symbol     String   
  rate       Decimal  
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
